# messages:

messages: 
	LIST=`find . -name \*.h -o -name \*.hh -o -name \*.H -o -name \*.hxx -o -name \*.hpp -o -name \*.cpp -o -name \*.cc -o -name \*.cxx -o -name \*.ecpp -o -name \*.C`; \
	if test -n "$$LIST"; then \
	 $(XGETTEXT) $$LIST -o $(podir)/l2swim.pot; \
	fi
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:

#>+ 3
l2swim.moc.cpp: $(srcdir)/l2swim.h
	$(MOC) $(srcdir)/l2swim.h -o l2swim.moc.cpp

#>+ 3
clean-metasources:
	-rm -f  l2swim.moc.cpp

#>+ 2
docs-am:

#>+ 6
force-reedit:
		cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  l2swim/Makefile
	cd $(top_srcdir) && perl admin/am_edit l2swim/Makefile.in


#>+ 11
l2swim.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/index.cpp $(srcdir)/l2swim.cpp $(srcdir)/main.cpp $(srcdir)/l2swim.moc.cpp 
	@echo 'creating l2swim.all_cpp.cpp ...'; \
	rm -f l2swim.all_cpp.files l2swim.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> l2swim.all_cpp.final; \
	for file in index.cpp l2swim.cpp main.cpp l2swim.moc.cpp ; do \
	  echo "#include \"$$file\"" >> l2swim.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> l2swim.all_cpp.final; \
	done; \
	cat l2swim.all_cpp.final l2swim.all_cpp.files > l2swim.all_cpp.cpp; \
	rm -f l2swim.all_cpp.final l2swim.all_cpp.files

#>+ 3
clean-final:
	-rm -f l2swim.all_cpp.cpp

#>+ 2
final:
	$(MAKE) l2swim_OBJECTS="$(l2swim_final_OBJECTS)" all-am
#>+ 2
final-install:
	$(MAKE) l2swim_OBJECTS="$(l2swim_final_OBJECTS)" install-am
#>+ 2
no-final:
	$(MAKE) l2swim_OBJECTS="$(l2swim_nofinal_OBJECTS)" all-am
#>+ 2
no-final-install:
	$(MAKE) l2swim_OBJECTS="$(l2swim_nofinal_OBJECTS)" install-am
#>+ 3
cvs-clean:
	$(MAKE) admindir=$(top_srcdir)/admin -f $(top_srcdir)/admin/Makefile.common cvs-clean

#>+ 3
kde-rpo-clean:
	-rm -f *.rpo
