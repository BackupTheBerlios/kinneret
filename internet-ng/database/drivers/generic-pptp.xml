<?xml version="1.0" encoding="UTF-8"?>
<modem>
    <name>PPtP ADSL Modem</name>
    <type>adsl</type>
    <interfaces>
        <interface>ethernet</interface>
    </interfaces>
    <vpn>
        <dialer>pptp</dialer>
        <address>10.0.0.138</address>
        <remotename>"10.0.0.138 RELAY_PPP1"</remotename>
    </vpn>
    <ethernet>
        <address>10.200.1.1</address>
        <mask>255.0.0.0</mask>
        <broadcast>255.255.255.255</broadcast>
    </ethernet>
    <supports>
        <name>Alcatel SpeedTouch Home</name>
    </supports>
</modem>
